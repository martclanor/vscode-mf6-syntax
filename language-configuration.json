{
  "folding": {
    "markers": {
      "start": "^(?i:BEGIN)",
      "end": "^(?i:END)"
    }
  },
  "comments": {
    "lineComment": "#"
  },
  "autoClosingPairs": [
    {
      "open": "'",
      "close": "'"
    }
  ],
  "surroundingPairs": [["'", "'"]],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(?i:BEGIN|k|DELR).*$"
  }
}
