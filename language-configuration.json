{
  "folding": {
    "markers": {
      "start": "^(?i:BEGIN)",
      "end": "^(?i:END)"
    }
  },
  "comments": {
    "lineComment": "#"
  },
  "autoClosingPairs": [
    {
      "open": "'",
      "close": "'"
    }
  ],
  "surroundingPairs": [["'", "'"]],
  "wordPattern": "\\b(?!\\d+\\b)[a-zA-Z0-9_-]+\\.[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)*\\b|(?<=')[^']*(?=')"
}
