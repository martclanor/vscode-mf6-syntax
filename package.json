{
  "name": "mf6-syntax",
  "displayName": "MODFLOW 6 Syntax",
  "version": "0.0.7",
  "description": "MODFLOW 6 input text file syntax highlighting support",
  "categories": [
    "Other"
  ],
  "keywords": [
    "mf6",
    "modflow6"
  ],
  "homepage": "https://github.com/martclanor/vscode-mf6-syntax/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/martclanor/vscode-mf6-syntax/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martclanor/vscode-mf6-syntax.git"
  },
  "license": "MIT",
  "publisher": "mart-clanor",
  "main": "./out/extension.js",
  "scripts": {
    "check:prettier": "prettier --check .",
    "compile:ts": "tsc -p ./",
    "format:prettier": "prettier --write .",
    "package": "vsce package",
    "publish": "vsce publish",
    "test:grammar": "vscode-tmgrammar-snap syntaxes/samples/*",
    "update:snapshot": "vscode-tmgrammar-snap --updateSnapshot syntaxes/samples/*"
  },
  "contributes": {
    "commands": [
      {
        "command": "mf6-syntax.mf6-ify",
        "title": "Syntax-highlight as MF6 file",
        "category": "MF6 Syntax"
      }
    ],
    "grammars": [
      {
        "language": "mf6",
        "scopeName": "source.mf6",
        "path": "./syntaxes/mf6.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "mf6",
        "aliases": [
          "MODFLOW 6"
        ],
        "extensions": [
          ".adv",
          ".api",
          ".ats",
          ".buy",
          ".cdb",
          ".chd",
          ".chfgwf",
          ".cnc",
          ".cnd",
          ".csub",
          ".ctp",
          ".cxs",
          ".dfw",
          ".dis",
          ".dis2d",
          ".disu",
          ".disv",
          ".disv1d",
          ".disv2d",
          ".drn",
          ".dsp",
          ".ems",
          ".esl",
          ".est",
          ".evp",
          ".evt",
          ".evta",
          ".flw",
          ".fmi",
          ".ghb",
          ".gnc",
          ".gwegwe",
          ".gwfgwe",
          ".gwfgwf",
          ".gwfgwt",
          ".gwfprt",
          ".gwtgwt",
          ".hfb",
          ".hpc",
          ".ic",
          ".ims",
          ".ist",
          ".lak",
          ".laktab",
          ".lke",
          ".lkt",
          ".maw",
          ".mip",
          ".mst",
          ".mve",
          ".mvr",
          ".mvt",
          ".mwe",
          ".mwt",
          ".nam",
          ".ncf",
          ".npf",
          ".obs",
          ".oc",
          ".olfgwf",
          ".pcp",
          ".prp",
          ".pts",
          ".rch",
          ".rcha",
          ".riv",
          ".sfe",
          ".sfr",
          ".sfrtab",
          ".sft",
          ".spc",
          ".spca",
          ".src",
          ".ssm",
          ".sto",
          ".tas",
          ".tdis",
          ".ts",
          ".tvk",
          ".tvs",
          ".uze",
          ".uzf",
          ".uzt",
          ".vsc",
          ".wel",
          ".zdg"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "images/icon.svg",
          "light": "images/icon.svg"
        }
      }
    ],
    "snippets": [
      {
        "language": "mf6",
        "path": "./snippets/mf6-snippets.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.2",
    "prettier": "^3.5.0",
    "typescript": "^5.7.3",
    "vscode-test": "^1.6.1",
    "vscode-tmgrammar-test": "^0.1.3"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "images/icon.png"
}
