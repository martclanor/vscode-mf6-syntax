            {
                "id": "gwf-wel",
                "extensions": [
                    ".wel"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "sim-nam",
                "filenames": [
                    "mfsim.nam"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "sim-tdis",
                "extensions": [
                    ".tdis"
                ],
                "configuration": "./language-configuration.json"
            },
            {
