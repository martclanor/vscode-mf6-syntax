{
    "name": "mf6-syntax",
    "displayName": "MODFLOW6 Syntax",
    "description": "MODFLOW6 input text file syntax highlighting support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.96.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "mf6",
        "modflow6"
    ],
    "scripts": {
        "build": "npm run compile:grammar",
        "compile:grammar": "for file in syntaxes/*.yaml; do js-yaml \"$file\" > \"${file%.yaml}.json\"; done"
    },
    "contributes": {
        "languages": [
            {
                "id": "common",
                "configuration": "./language-configuration.json"
            },
            {
                "id": "gwf-chd",
                "extensions": [
                    ".chd"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "gwf-dis",
                "extensions": [
                    ".dis"
                ],
                "configuration": "./language-configuration.json"
            },
            {
