{
    "name": "mf6-syntax",
    "displayName": "MODFLOW 6 Syntax",
    "description": "MODFLOW 6 input text file syntax highlighting support",
    "version": "0.0.2",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.96.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "mf6",
        "modflow6"
    ],
    "homepage": "https://github.com/martclanor/vscode-mf6-syntax/blob/main/README.md",
    "bugs": {
        "url": "https://github.com/martclanor/vscode-mf6-syntax/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/martclanor/vscode-mf6-syntax.git"
    },
    "license": "MIT",
    "publisher": "mart-clanor",
    "scripts": {
        "build": "npm run compile:grammar",
        "compile:grammar": "for file in syntaxes/*.yaml; do js-yaml \"$file\" > \"${file%.yaml}.json\"; done"
    },
    "contributes": {
        "languages": [
            {
                "id": "common"
            },
            {
                "id": "mf6",
                "aliases": [
                    "MODFLOW 6"
                ],
                "extensions": [
                    ".adv",
                    ".api",
                    ".ats",
                    ".buy",
                    ".chd",
                    ".cnc",
                    ".cnd",
                    ".csub",
                    ".ctp",
                    ".dis",
                    ".disu",
                    ".disv",
                    ".drn",
                    ".dsp",
                    ".ems",
                    ".esl",
                    ".est",
                    ".evt",
                    ".evta",
                    ".fmi",
                    ".ghb",
                    ".gnc",
                    ".gwegwe",
                    ".gwfgwe",
                    ".gwfgwf",
                    ".gwfgwt",
                    ".gwfprt",
                    ".gwtgwt",
                    ".hfb",
                    ".hpc",
                    ".ic",
                    ".ims",
                    ".ist",
                    ".lak",
                    ".laktab",
                    ".lke",
                    ".lkt",
                    ".maw",
                    ".mip",
                    ".mst",
                    ".mve",
                    ".mvr",
                    ".mvt",
                    ".mwe",
                    ".mwt",
                    ".nam",
                    ".ncf",
                    ".npf",
                    ".obs",
                    ".oc",
                    ".prp",
                    ".pts",
                    ".rch",
                    ".rcha",
                    ".riv",
                    ".sfe",
                    ".sfr",
                    ".sfrtab",
                    ".sft",
                    ".spc",
                    ".spca",
                    ".src",
                    ".ssm",
                    ".sto",
                    ".tas",
                    ".tdis",
                    ".ts",
                    ".tvk",
                    ".tvs",
                    ".uze",
                    ".uzf",
                    ".uzt",
                    ".vsc",
                    ".wel"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "common",
                "scopeName": "source.mf6.common",
                "path": "./syntaxes/common.tmLanguage.json"
            },
            {
                "language": "mf6",
                "scopeName": "source.mf6.all",
                "path": "./syntaxes/all.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "mf6",
                "path": "./snippets/mf6-snippets.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}
